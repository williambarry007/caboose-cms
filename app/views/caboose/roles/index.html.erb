<h1>Roles</h1>
<p><a href='/admin/roles/new' class="caboose-btn">New Role</a></p>

<table class='data' style="width:96%;">
  <tr>
    <th>Name</th>
  </tr>
<% @roles.each do |r| %> 
  <tr onclick="window.location='/admin/roles/<%= r['value'] %>/edit';">
    <td><%= r['text'] %></td>
  </tr>
<% end %>
</table>

